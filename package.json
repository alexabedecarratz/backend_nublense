{
  "name": "nublense",
  "private": true,
  "version": "1.0.0",
  "type": "commonjs",
  "scripts": {
    "start": "node src/index.js",
    "deploy": "node start.js",
    "dev": "nodemon src/index.js",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:drop": "sequelize db:drop",
    "db:create": "sequelize db:create",
    "db:reset": "run-s db:drop db:create"
  },
  "dependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@koa/cors": "^4.0.0",
    "bcrypt": "npm:bcrypt@5.0.0",
    "dotenv": "^16.3.1",
    "init": "^0.1.2",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "koa-jwt": "^4.0.4",
    "koa-logger": "^3.2.1",
    "koa-router": "^12.0.1",
    "mysql2": "^3.6.5",
    "nodemon": "^3.0.1",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.15.0",
    "sequelize": "^6.35.1",
    "sequelize-cli": "^6.6.1"
  },
  "devDependencies": {
    "@babel/node": "^7.22.19",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "bite": "^0.1.0-4",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "vite": "^4.4.9"
  },
  "main": "index.js",
  "repository": "https://github.com/IIC2513/grupo_Nublense_backend.git",
  "author": "FlorenciaOnetto <florencia.onetto@uc.cl>",
  "license": "MIT"
}
